{
	"name": "ginkgo-outline",
	"displayName": "Ginkgo Outline",
	"description": "Display an outline of the ginkgo containers and specs in a file, and enable navigation to containers and specs using QuickView.",
	"version": "0.0.1",
	"publisher": "dlipovetsky",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ginkgooutline.GotoSymbolInEditor",
		"onView:ginkgooutline.views.outline"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ginkgooutline.GotoSymbolInEditor",
				"title": "Ginkgo: Go to symbol"
			}
		],
		"viewsContainers": {
			"panel": [
				{
					"id": "ginkgooutline",
					"title": "Ginkgo Panel",
					"icon": ""
				}
			]
		},
		"views": {
			"ginkgooutline": [
				{
					"id": "ginkgooutline.views.outline",
					"name": "Ginkgo Outline",
					"icon": ""
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "ts-mocha src/**/*.unit.test.ts && node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.14",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"chai": "^4.2.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-mocha": "^8.0.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}