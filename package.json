{
	"name": "ginkgo-outline",
	"displayName": "Ginkgo Outline",
	"description": "Display an outline of the ginkgo containers and specs in a file, and enable navigation to containers and specs using QuickView.",
	"version": "0.0.1",
	"publisher": "dlipovetsky",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ginkgooutline.GotoSymbolInEditor",
		"onView:ginkgooutline.views.outline"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ginkgooutline.GotoSymbolInEditor",
				"title": "Ginkgo: Go to symbol"
			}
		],
		"views": {
			"test": [
				{
					"id": "ginkgooutline.views.outline",
					"name": "Ginkgo Outline"
				}
			]
		},
		"configuration": {
			"title": "Ginkgo Outline",
			"properties": {
				"ginkgooutline.updateOn": {
					"type": "string",
					"default": "onType",
					"enum": [ "onSave", "onType" ],
					"enumDescriptions": [ "Update on file save", "Update as you type"],
					"description": "When to update the outline view."
				},
				"ginkgooutline.updateOnTypeDelay": {
					"type": "number",
					"default": 1000,
					"minimum": 0,
					"description": "Minimum time, in milliseconds, to wait before running `ginkgo outline` after a document update. Smaller values result in repeated calls to `ginkgo outline` as you type."
				},
				"ginkgooutline.ginkgoPath": {
					"type": "string",
					"default": "ginkgo",
					"description": "Path to ginkgo executable."
				},
				"ginkgooutline.doubleClickThreshold": {
					"type": "number",
					"default": 400,
					"minimum": 0,
					"description": "Maximum time, in milliseconds, between two clicks in a double click."
				},
				"ginkgooutline.cacheTTL": {
					"type": "number",
					"default": 3600,
					"minimum": 0,
					"description": "Maximum time, in seconds, to cache the result of `ginkgo outline` for a specific document version."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "ts-mocha src/**/*.unit.test.ts && node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.14",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"chai": "^4.2.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-mocha": "^8.0.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}